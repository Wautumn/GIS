              url:"http://10.60.42.201:6447/arcgis/rest/services/%E4%BA%BA%E6%B0%91%E5%B9%BF%E5%9C%BA/MapServer/1",
              outFields: ["*"],
              popupTemplate: template
          });
          var featureLayer2 = new FeatureLayer({
              // url: "https://10.60.42.201:6442/arcgis/rest/services/culture_poi/MapServer/1"
              url:"http://10.60.42.201:6447/arcgis/rest/services/%E4%BA%BA%E6%B0%91%E5%B9%BF%E5%9C%BA/MapServer/2",
              outFields: ["*"],
              popupTemplate: template
          });
          view.then(function() {
              featureLayer.then(function() {
